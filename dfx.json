{
  "version": 1,
  "dfx": "0.10.2-btcbeta.0",
  "canisters": {
    "bitcoin_integration_demo": {
      "type": "custom",
      "package": "bitcoin_integration_demo",
      "candid": "src/bitcoin_integration_demo/bitcoin_integration_demo.did",
      "wasm": "target/wasm32-unknown-unknown/release/bitcoin_integration_demo.wasm",
      "build": "src/bitcoin_integration_demo/build.sh"
    },
    "bitcoin_wallet_assets": {
      "type": "assets",
      "dependencies": [
        "bitcoin_integration_demo"
      ],
      "frontend": {
        "entrypoint": "dist/frontend/src/index.html"
      },
      "source": [
        "./dist/frontend/"
      ]
    },
    "ecdsa": {
      "type": "custom",
      "package": "ecdsa",
      "candid": "ecdsa.did",
      "wasm": "ecdsa.wasm"
    }
  },
  "defaults": {
    "bitcoin": {
      "enabled": true,
      "nodes": ["127.0.0.1:18444"]
    },
    "build": {
      "packtool": "",
      "args": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  }
}
